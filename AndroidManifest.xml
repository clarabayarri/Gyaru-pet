<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.amieggs.gyaru"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="8" />

    <application
        android:icon="@drawable/gyaru_icon"
        android:label="@string/app_name" >
        <activity
            android:label="@string/app_name"
            android:name=".MainActivity" android:theme="@style/GyaruTheme">
            <intent-filter >
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.amieggs.gyaru.GyaruActivity" android:screenOrientation="portrait" android:theme="@style/GyaruTheme"/>
        <activity android:name="com.amieggs.gyaru.AboutActivity" android:screenOrientation="portrait" android:theme="@style/GyaruTheme"/>
    </application>

</manifest>